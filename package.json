    "helmet": "^8.1.0",
    "express-rate-limit": "^7.0.0",
    "dotenv": "^16.4.5",
    "prom-client": "^15.1.3",
    "winston": "^3.10.0",
    "morgan": "^1.10.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "supertest": "^6.3.4",
    "mongodb-memory-server": "^8.14.0",
    "nodemon": "^3.0.3"
  }
