{
  "name": "lab5",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "start": "node APP/src/server.js",
    "dev": "nodemon APP/src/server.js",
    "test": "jest --detectOpenHandles --forceExit",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration --detectOpenHandles --forceExit",
